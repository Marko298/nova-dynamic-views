{
    "name": "Marko298/nova-dynamic-views",
    "description": "A tool for easier overwriting custom-header and toolbaars in Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Bernhardh\\NovaDynamicViews\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bernhardh\\NovaDynamicViews\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
